#!/usr/bin/env node
const program = require('commander')
const package = require('../package.json');
program.version(package.version, '-v, --version')
	.usage('<command> [项目名称]')
	.command('lev2', 'lev2')
    .command('lev2-init', 'lev2-init')
    .command('lev2-server', 'lev2-server')
	.parse(process.argv)