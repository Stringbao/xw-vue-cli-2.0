import <%= data.servicesClassName %> from "@services/<%= data.servicesPath%>";

export default {
    namespaced: true,
    state:{
    <%_ data.stateKeys.forEach(function(row){ -%>
        <%= row %>: [],
    <%_ _%><% }) -%>
    },
    mutations:{
    <%_ data.mutations.forEach(function(row){ -%>
        <%= row.fnName %>(state,data){
            if(data && data instanceof Array){
                state.<%= row.stateKey %> = data;
            }
        },
    <%_ _%><% }) -%>
    },
    actions:{
    <%_ data.actions.forEach(function(row){ -%>
        <%= row.fnName %>(context,cb){
            if(context.state.<%= row.stateKey %>.length >0){
                return;
            }
            <%= data.servicesClassName %>.<%= row.serviceFnName %>().then((res)=>{
                context.commit("<%= row.commitFnName %>",res.data.data);
                cb && cb(res);
            });
        },
    <%_ _%><% }) -%>
    }
}
