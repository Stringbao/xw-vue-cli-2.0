import Ajax from "@util/http.js";
import KEYS from "@constant/index.js";

const map = {
    <%_ data.map.forEach(function(row){ -%>
      <%= row.name %>: '<%= row.url%>',
   <%_ _%><% }) -%>
}

export default {
    save(model) {
        return model.id ? this.update(model) : this.create(model);
    },
    <%_ data.fns.forEach(function(row){ -%>
        <%= row.fnName %>(model){
        <%_ if (row.isEnum) { -%>
            return Promise.resolve({data:<%= row.url%>});
        <%_ _%><% }else { -%>
            let params = {};
        <%_ if (row.isGetReq) { -%>
            return Ajax.get(map.<%= row.mapKey%>, params);
            <%_ _%><% } -%>
        <%_ _%><% }else { -%>
            return Ajax.post(map.<%= row.mapKey%>, params);
        <%_ _%><% } -%>
        },
    <%_ _%><% }) -%>
}