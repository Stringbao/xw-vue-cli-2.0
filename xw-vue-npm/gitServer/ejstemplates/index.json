            "displayValue": "value",
            "dataSource": "salesRouteA",
            "on": true,
            "required": true
          }
        ]
      },
      {
        "PageName": "save.vue",
        "type": "save",
        "config": {
          "model": [{
            "label": "角色类型",
            "type": "select",
            "field": "roleType",
            "displayName": "name",
            "displayValue": "value",
